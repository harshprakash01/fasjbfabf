{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],

  "rewrites": [
    { "source": "/listen/(.*)", "destination": "/listen" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],

    "env": {
      "NEXT_PUBLIC_API_URL": "https://api.example.com"
    }
  }
  